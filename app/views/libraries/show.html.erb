<a href="/libraries">Library Index</a>

<h1><%= @library.name %></h1>
<h3>Public?</h3>
  <%if @library.public == nil %>
    <p><%= false %></p>
    <% else %>
    <p><%= true %></p>
  <% end %>
<h3>Years Opened:</h3>
<p><%= @library.years_opened %></p>
<a href="/libraries/<%= @library.id %>/edit">Update Library</a><br><br>
<a><%= link_to "Delete Library", "/libraries/#{@library.id}", method: :delete %></a>
